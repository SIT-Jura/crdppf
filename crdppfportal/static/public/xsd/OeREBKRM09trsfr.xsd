    <xsd:extension base="OeREBKRM09vs:VorschriftenType">
      <xsd:sequence>
        <xsd:element ref="DarstellungsDienst" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="Eigentumsbeschraenkung" minOccurs="0" maxOccurs="unbounded"/>
